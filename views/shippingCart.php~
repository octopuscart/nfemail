if (isset($_POST['orderConfirm'])) {
    $qun = $_POST['totalQuantity'];
    $pri = $_POST['totalPrice'];
    $arr = $_POST['allCartId'];
    $cartIdss = explode(",", $arr);
    $user_id = $_POST['user_id'];
    
    if ($_POST['s_address1'] != '') {
          $lastids = $authobj->bill_ship_insert($_POST, $_SESSION['user_id']);
          $billId = $lastids[0];
          $shipId = $lastids[1];
          $order_id = $cartprd->insertInOrderTable($qun, $pri, $cartIdss, $user_id, $billId, $shipId);
          header('location: shop_order_information.php?order_id=' . $order_id);
    } 
    else {
       
        $billId = $_POST['billing_id'];
        $shipId = $_POST['shipping_id'];
        $order_id = $cartprd->insertInOrderTable($qun, $pri, $cartIdss, $user_id, $billId, $shipId);
        header('location: shop_order_information.php?order_id=' . $order_id);
    }
}
